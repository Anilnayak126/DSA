   "source": [
    "Reverse an Array"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "[5, 5, 4, 3, 5, 2, 1]"
      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def reverse_array(array):\n",
    "    s = 0\n",
    "    e = len(array)-1\n",
    "    \n",
    "    while(s<e):\n",
    "        array[s], array[e] = array[e], array[s]\n",
    "        s += 1\n",
    "        e -= 1\n",
    "    return array\n",
    "reverse_array([1,2,5,3,4,5,5])\n"
   ]
  }
 ],
